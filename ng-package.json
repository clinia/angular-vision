{
  "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
  "dest": "./dist/angular-vision",
  "lib": {
    "flatModuleFile": "angular-vision",
    "entryFile": "./src/index.ts",
    "umdId": "cvi",
    "amdId": "cvi",
    "umdModuleIds": {
      "clinia": "clinia",
      "clinia/index": "clinia",
      "@clinia/vision/es": "vision",
      "@clinia/vision/es/connectors": "vision.connectors",
      "lodash": "_",
      "querystring-es3/encode": "qs.encode",
      "@clinia/search-helper": "searchHelper"
    }
  },
  "whitelistedNonPeerDependencies": [
    "clinia",
    "@clinia/search-helper",
    "@clinia/vision",
    "lodash",
    "querystring-es3"
  ]
}
