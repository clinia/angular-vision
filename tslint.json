{
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-submodule-imports": false,
    "import-name": false,
    "object-shorthand-properties-first": false,
    "directive-selector": false,
    "component-selector": false
  },
  "cliOptions": {
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
}
